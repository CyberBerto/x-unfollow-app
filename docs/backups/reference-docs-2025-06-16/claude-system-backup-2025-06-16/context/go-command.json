{
  "go_command": {
    "trigger": "GO",
    "description": "Load complete context and display current state for session startup",
    "implementation": "active",
    "auto_actions": [
      "Get current timestamp using date command",
      "Read all .claude/context/ files",
      "Reference docs/reference/ folder for essential information", 
      "Display current layer status and priorities",
      "Show immediate next steps",
      "Confirm context loaded and ready"
    ],
    "timestamp_format": "date \"+%Y-%m-%d-%H-%M\"",
    "context_sources": {
      "claude_context": ".claude/context/",
      "obsidian_reference": "docs/reference/",
      "key_reference_files": [
        "docs/reference/Session-Command-Behavior.md",
        "docs/reference/Development-Principles.md", 
        "docs/reference/Project-Structure-Standards.md",
        "docs/reference/Workflow-Standards.md"
      ]
    },
    "display_format": {
      "header": "🚀 X Unfollow App - Session Ready",
      "sections": [
        "📊 Project Status (Layer X of 5, Y% complete)",
        "🎯 Current Focus (Layer objective and implementation point)",
        "⚡ Next Priorities (Top 3 immediate tasks)",
        "📁 Key Context (Important files, functions, locations)",
        "✅ Ready Confirmation"
      ]
    }
  }
}